<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/rectangle_dialog_go_to_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_dialog_go_to_settings"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/settings_icon_dialog_permission_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/settings_icon_dialog_permission_settings"
        app:layout_constraintBottom_toBottomOf="@id/rectangle_dialog_go_to_settings"
        app:layout_constraintEnd_toEndOf="@id/rectangle_dialog_go_to_settings"
        app:layout_constraintStart_toStartOf="@id/rectangle_dialog_go_to_settings"
        app:layout_constraintTop_toTopOf="@id/rectangle_dialog_go_to_settings"
        app:layout_constraintVertical_bias="0.32" />

    <ImageView
        android:id="@+id/dialog_permission_settings_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/dialog_permission_settings_content"
        app:layout_constraintBottom_toBottomOf="@id/rectangle_dialog_go_to_settings"
        app:layout_constraintEnd_toEndOf="@id/settings_icon_dialog_permission_settings"
        app:layout_constraintStart_toStartOf="@id/settings_icon_dialog_permission_settings"
        app:layout_constraintTop_toBottomOf="@id/settings_icon_dialog_permission_settings"
        app:layout_constraintVertical_bias="0.24000001" />

    <ImageView
        android:id="@+id/go_to_settings_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/go_to_settings_button"
        app:layout_constraintTop_toBottomOf="@id/dialog_permission_settings_content"
        app:layout_constraintStart_toStartOf="@id/dialog_permission_settings_content"
        app:layout_constraintEnd_toEndOf="@id/dialog_permission_settings_content"
        app:layout_constraintBottom_toBottomOf="@id/rectangle_dialog_go_to_settings"
        />

</androidx.constraintlayout.widget.ConstraintLayout>